pipeline {
    agent any
    parameters {
        string defaultValue: "", name: 'DEPLOY_NUMBER', trim: true
    }
    stages {

            stage("Deploy") {
                        steps {
                        sh "kubectl apply -f LoadBalancer/deploy.yaml --env-file .env "
                        sh "kubectl apply -f LoadBalancer/service.yaml"
                        }
                    }
        }
}